<h2>配送先編集</h2>
<%= form_with model: @address,url: edit_public_address_path,method: :patch do |f| %>
    <h4>郵便番号（ハイフンなし）</h4>
    <%= f.text_field :ordered_postal_code %>
    <h4>住所</h4>
    <%= f.text_field :ordered_address %>
    <h4>宛名</h4>
    <%= f.text_field :receriver_name %>
    <%= f.submit "変更内容を保存" %>
<% end %>
